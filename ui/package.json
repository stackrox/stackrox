{
    "name": "@stackrox/ui",
    "description": "Deprecated top-level package.json for StackRox UI",
    "version": "0.0.0",
    "license": "UNLICENSED",
    "private": true,
    "engines": {
        "node": ">=18.0.0"
    },
    "scripts": {
        "clean": "rm -rf build test-results; cd apps/platform; yarn clean",
        "start": "cd apps/platform; CI=true yarn start",
        "lint": "cd apps/platform; yarn lint",
        "test": "cd apps/platform; CI=true yarn test",
        "test-e2e": "cd apps/platform; TEST_RESULTS_OUTPUT_DIR=../../test-results yarn test-e2e",
        "test-component": "cd apps/platform; yarn test-component",
        "posttest-e2e:coverage": "mv apps/platform/coverage/ ./test-results/artifacts/",
        "prebuild": "yarn clean",
        "build": "cd apps/platform; yarn build",
        "postbuild": "mv apps/platform/build/ .",
        "connect": "../scripts/connect-ui.sh ${1}",
        "deploy": "../deploy/k8s/deploy.sh",
        "deploy-local": "../deploy/k8s/deploy-local.sh",
        "forward": "../scripts/port-forward-ui.sh"
    }
}
