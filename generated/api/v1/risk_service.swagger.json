  "paths": {
    "/v1/risk/deployment/reset-all": {
      "post": {
        "summary": "ResetAllDeploymentRisks removes all user ranking adjustments for all deployments",
        "operationId": "RiskService_ResetAllDeploymentRisks",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/v1ResetAllRisksResponse"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/googlerpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/v1ResetAllRisksRequest"
            }
          }
        ],
        "tags": [
          "RiskService"
        ]
      }
    },
    "/v1/risk/deployment/{deploymentId}/position": {
      "post": {
        "summary": "ChangeDeploymentRiskPosition adjusts a deployment's risk ranking\nby calculating effective score as average of neighbor deployments",
        "operationId": "RiskService_ChangeDeploymentRiskPosition",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/v1RiskAdjustmentResponse"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/googlerpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "deploymentId",
            "description": "Deployment ID to adjust",
            "in": "path",
            "required": true,
            "type": "string"
          },
          {
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/RiskServiceChangeDeploymentRiskPositionBody"
            }
          }
        ],
        "tags": [
          "RiskService"
        ]
      }
    },
    "/v1/risk/deployment/{deploymentId}/reset": {
      "post": {
        "summary": "ResetDeploymentRisk removes user ranking adjustments and returns to the original ML-calculated score",
        "operationId": "RiskService_ResetDeploymentRisk",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/v1RiskAdjustmentResponse"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/googlerpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "deploymentId",
            "description": "Deployment ID to reset",
            "in": "path",
            "required": true,
            "type": "string"
          },
          {
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/RiskServiceResetDeploymentRiskBody"
            }
          }
        ],
        "tags": [
          "RiskService"
        ]
      }
    }
  },
