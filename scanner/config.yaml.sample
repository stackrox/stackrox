http_listen_addr: 127.0.0.1:9443
grpc_listen_addr: 127.0.0.1:8443
indexer:
  enable: true
  database:
    conn_string: "host=/var/run/postgresql"
    password_file: ""
  get_layer_timeout: 1m
  repo:
    rhel-repository-scanner:
        repo2cpe_mapping_url: https://sensor.stackrox.svc/scanner-v4/definitions?file=repo2cpe
matcher:
  enable: true
  database:
    conn_string: "host=/var/run/postgresql"
    password_file: ""
mtls:
  certs_dir: ""
log_level: info
